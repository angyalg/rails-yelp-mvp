<h1>Restaurant/Index</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Phone Number</th>
      <th>Category</th>
    </tr>
  </thead>
  <tbody>
  <% @restaurants.each do |r| %>
    <tr>
      <td><%= r.name %></td>
      <td><%= r.address %></td>
      <td><%= r.phone_number %></td>
      <td><%= r.category %></td>
    </tr>
    <% end %>
  </tbody>
</table>
